<odoo>
    <record id="group_sale_product_pack_modification" model="res.groups">
        <field name="name">Allow to modify Sale Order Lines Pack Product</field>
        <field name="comment">Allow to modify Sale Order Lines Pack Product</field>
        <field name="implied_ids" eval="[(4, ref('sales_team.group_sale_salesman'))]" />
    </record>

    <record model="ir.model.access" id="sale_product_pack_line_modification_user">
        <field
            name="name"
        >sale_product_pack_line_modification (in sale_product_pack_line_modification)</field>
        <field
            name="model_id"
            ref="sale_product_pack_line_modification.model_sale_product_pack_line_modification"
        />
        <field name="group_id" ref="group_sale_product_pack_modification" />
        <field name="perm_read" eval="1" />
        <field name="perm_create" eval="1" />
        <field name="perm_write" eval="1" />
        <field name="perm_unlink" eval="1" />
    </record>
</odoo>
